{
  "name": "@twinlogix/typetta",
  "version": "1.0.0-beta.15",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --detectOpenHandles --no-cache --forceExit && make-coverage-badge",
    "generate": "npm run generate-mongodb && npm run generate-sql && npm run generate-demo && npm run generate-id",
    "generate-mongodb": "graphql-codegen --config tests/mongodb/codegen.yml",
    "generate-sql": "graphql-codegen --config tests/sql/codegen.yml",
    "generate-demo": "graphql-codegen --config tests/demo/codegen.yml",
    "generate-id": "graphql-codegen --config tests/id_generator/codegen.yml",
    "typedoc": "typedoc --excludePrivate --excludeProtected --excludeInternal --githubPages false --out docs/typedoc src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twinlogix/typetta.git"
  },
  "license": "Apache-2.0",
  "maintainers": [
    "Mattia Minotti <mattia@twinlogix.com>",
    "Edoardo barbieri <edoardo@twinlogix.com>"
  ],
  "engines": {
    "node": ">=12.6",
    "pnpm": ">=6.14.1"
  },
  "bugs": {
    "url": "https://github.com/twinlogix/typetta/issues"
  },
  "devDependencies": {
    "@graphql-codegen/add": "3.1.0",
    "@trivago/prettier-plugin-sort-imports": "3.1.1",
    "@types/jest": "27.4.0",
    "@types/node": "16.11.20",
    "@types/object-hash": "1.3.4",
    "@types/sha256": "0.2.0",
    "@types/uuid": "8.3.4",
    "apollo-server": "3.6.1",
    "apollo-server-testing": "2.25.3",
    "bignumber.js": "9.0.2",
    "jest": "27.4.7",
    "make-coverage-badge": "1.2.0",
    "mongodb-memory-server": "8.0.2",
    "sha256": "0.2.0",
    "sqlite3": "5.0.2",
    "ts-jest": "27.1.3",
    "tslint": "6.1.3",
    "typedoc": "0.22.10",
    "typescript": "4.5.4",
    "uuid": "8.3.2"
  },
  "dependencies": {
    "@graphql-codegen/cli": "2.3.1",
    "@graphql-codegen/plugin-helpers": "2.3.1",
    "@graphql-codegen/schema-ast": "2.4.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/visitor-plugin-common": "2.5.1",
    "dataloader": "2.0.0",
    "graphql": "15.8.0",
    "is-plain-object": "5.0.0",
    "knex": "0.95.15",
    "mongodb": "4.3.0",
    "object-hash": "2.2.0",
    "type-fest": "0.21.3"
  },
  "jest": {
    "rootDir": "./",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "coveragePathIgnorePatterns": [
      "src/generation/",
      "tests/"
    ],
    "coverageReporters": [
      "json-summary"
    ]
  },
  "homepage": "https://twinlogix.github.io/typetta/"
}
